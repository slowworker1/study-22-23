#include <stdio.h>

int main()
{
	int i=1,a=1,b=1;
	while (i<=10)
	{
		printf("%d ",i);
		i++;
	}
	printf("\n");
	
	while (a<=10)
	{
		if (a == 5)
		    break; //跳出整个循环 
		printf("%d ",a);
		a++;
	}
	printf("\n");

    while (b<=10)
	{
		b++;
		if (b==5)
		    continue;//跳出本次循环 
		printf("%d ",b);
	}
	printf("\n");
	
//	int ch=0;
//    ch=getchar();//getchar 相当于输入 
//	putchar(ch);//putchar 相当于输出 
	
	while((ch=getchar()) != EOF) //EOF == ctrl + Z 
	{
		putchar(ch);
	}
	
	return 0;
}
